        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".pager.AnnActivity" />
        <activity android:name=".pager.ServiceActivity" />
        <activity android:name=".pager.FindActivity" />
        <activity android:name=".pager.BizActivity" />
